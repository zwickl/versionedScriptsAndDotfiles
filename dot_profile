function mark(){
    if [ ! "$#" -eq 1 ];then
        echo "Enter a name for this mark"
        return
    fi
    pwd > $HOME/.markedDirs/$1
}

function marks(){
    for DIR in $HOME/.markedDirs/*
    do
        echo
        echo ${DIR##*/}
        cat $DIR
    done
}

function cdm(){
    if [ ! "$#" -eq 1 ];then
        echo
        echo "Defined marks"
        for DIR in $HOME/.markedDirs/*
        do
            echo
            echo ${DIR##*/}
            cat $DIR
        done
        #echo "Enter a named mark"
        return
    fi
    cd `cat $HOME/.markedDirs/$1`
}

