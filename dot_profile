function vm(){
    if [ $# -eq 2 ];then
        echo $2 > $HOME/.vmark$1
    elif [ $# -eq 1 ];then
        vi `cat $HOME/.vmark$1`
    fi
}

function countFastaSeqs(){
    for i in $@
    do
        echo $i
        grep '^>' $i | wc -l
    done
}

function occurenceCount(){
    PATT=$1
    shift 1
    grep $PATT $@ | wc -l
}

function fileCount(){
    ls $@ | wc -l
}

