fi


function dotdu(){
for DIR in `sudo ls -a  $1`
do
    if [[ $DIR == "."* ]]; then
        if [[ $DIR == "." ]]; then
            continue
        fi
        if [[ $DIR == ".." ]]; then
            continue
        fi
    sudo du -hs $1/$DIR
    fi
done
}


function activate(){
    VENV_PATH=$VENVV/$1/
    export VENV_PATH
    if [ ! -d $VENV_PATH ]; then
        drectory $VENV_OPATH not found for virtual environment $1
    fi

# source $VENV_PATH/bin/activate  # commented out by conda initialize
}


